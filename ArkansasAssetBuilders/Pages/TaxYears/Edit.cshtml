@page
@model ArkansasAssetBuilders.Pages.TaxYears.EditModel

@{
    ViewData["Title"] = "Edit an Existing Tax Year";
}

<h1>Edit an Existing Tax Year</h1>

<h4>TaxYear</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TaxYear.TaxYearID" class="control-label"></label>
                <input asp-for="TaxYear.TaxYearID" class="form-control" />
                <span asp-validation-for="TaxYear.TaxYearID" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="TaxYear.ID" />
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
